<?xml version="1.0" encoding="utf-8"?>
<merge xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="44dp">

    <ImageView
        android:id="@+id/layout_page_header_backImage"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:layout_centerVertical="true"
        android:paddingEnd="@dimen/view_paddingRight_15"
        android:paddingLeft="@dimen/view_paddingLeft_15"
        android:paddingRight="@dimen/view_paddingRight_15"
        android:paddingStart="@dimen/view_paddingLeft_15"
        android:scaleType="center" />

    <TextView
        android:id="@+id/layout_page_header_name"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_centerInParent="true" />

    <TextView
        android:id="@+id/layout_page_header_operate"
        android:layout_width="wrap_content"
        android:layout_height="match_parent"
        android:layout_alignParentEnd="true"
        android:layout_alignParentRight="true"
        android:gravity="center_vertical"
        android:paddingLeft="@dimen/view_padding_15"
        android:paddingRight="@dimen/view_padding_15"
        android:text="完成" />

    <View
        android:layout_width="match_parent"
        android:layout_height="1dp"
        android:layout_alignParentBottom="true"
        android:background="@color/skx_f3f3f3" />

</merge>
